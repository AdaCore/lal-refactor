with Ada.Text_IO;

procedure Main is
   Tmp2 : Integer;
   Tmp3 : Integer := 1 + 1;
begin
   if (2 + 2) / 2 = 2 then
      Ada.Text_IO.Put_Line ("Yes");
   end if;

   if Tmp2 / 2 = 1 then
      Ada.Text_IO.Put_Line ("Yes");
   end if;

   Tmp3 := Tmp3 + 2;
   if Tmp3 / 2 = 2 then
      Ada.Text_IO.Put_Line ("Yes");
   end if;
end Main;
